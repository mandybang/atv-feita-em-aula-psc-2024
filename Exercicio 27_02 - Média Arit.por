programa {
  funcao inicio() {
    inteiro numero1, numero2, numero3
    real x
    escreva ("Escreva um número: ")
    leia (numero1)
    escreva ("Escreva outro numero: ")
    leia(numero2)
    escreva ("Escreva o ultimo número: ")
    leia(numero3)
    x=(numero1+numero2+numero3)/3
    escreva ("Resultado:" + x)
  }
}
